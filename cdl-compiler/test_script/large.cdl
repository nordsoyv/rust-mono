    removeEmptySeries: true
    removeEmptyCategories: true
    size: halfwidth

    chartMargin {
      right: 60
      bottom: 50
    }

    series #series {
      chart bar #barChart {
        maxBarSize: 50
      }
      value: count(amTable:Trigger)
    }

    axis primary #primaryAxis {
      axisLine: true
      tickLine: true
      minValue: 0
      maxValue: -1
