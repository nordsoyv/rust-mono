    gridLines: horizontal
    removeEmptyCategories: true

    filter expression {
      value: amTable:SystemStatus = "closed"
    }

    chartMargin {
      right: 60
      bottom: 50
    }
    chart area {
      lineType: monotone

    }

    series {
      label: "Average Days to Close"
      value: median(amTable:daysToClose)
    }

    category cut {
      value: amTable:Workflow
    }

