      axisLine: true
      tickLine: true
    }
    axis category #categoryAxis {
      axis secondary #secondaryAxis {
        hide: true
      }
      orientation: "-45"
    }
    axis secondary #secondaryAxis {
      hide: true
    }
    removeEmptySeries: true
  } //end widget
